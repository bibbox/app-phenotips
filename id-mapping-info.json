{
	"documentation": "https://phenotips.org/DevGuide/RESTfulAPI",
	"mappings": {
		"PATIENT": {
			"human_readable": ["var appname = &&an;",
				"var p1      = &&p1;",
				"var result  = 'https://' + appname + '/bin/ + p1;",
				"println(result);"
			],

			"get_all": ["var appname = &&an;",
				"var result  = 'https://' + appname + '/rest/patients';",
				"println(result);"
			],

			"get_info": ["var appname = &&an;",
				"var p1      = &&p1;",
				"var result  = 'https://' + appname + '/rest/patients/' + p1;",
				"println(result);"
			]
		}
	}
}