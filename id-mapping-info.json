{
	"documentation": ["https://phenotips.org/DevGuide/RESTfulAPI"],
	"mappings": {
		"SUBJECT": {
			"human_readable": [
                "(",
                "function() {",
                "var appID = '&&aid';",
				"var localId1      = '&&id1';",
				"var result        = 'http://' + appID + '/bin/' + localId1;",
				"return result;",
                "}",
                ")()"
			],

			"get_all": [
                "(",
                "function() {",
                "var appID = '&&aid';",
				"var result  = 'http://' + appID + '/rest/patients';",
                "return result;",
                "}",
                ")()"
			],

			"get_info": [
                "(",
                "function() {",
                "var appID = '&&aid';",
				"var localId1      = '&&id1';",
				"var result        = 'http://' + appID + '/rest/patients/' + localId1;",
				"return result;",
                "}",
                ")()"
			]
		}
	}
}